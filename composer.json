{
  "name": "linkprofit-cpa/tracker-client",
  "description": "Tracker API client",
  "keywords": [
    "tracker",
    "API"
  ],
  "homepage": "https://github.com/linkprofit-cpa/tracker-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Khatin Konstantin",
      "email": "phrlog@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "psr/simple-cache": "^1.0",
    "guzzlehttp/guzzle": "^6.3",
    "duncan3dc/cache": "dev-psr-1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "mikey179/vfsStream": "~1",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/linkprofit-cpa/cache.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "linkprofit\\Tracker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "linkprofit\\Tracker\\tests\\": "tests/"
    }
  }
}
